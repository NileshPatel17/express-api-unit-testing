{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": ["prettier", "airbnb-base"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "consistent-return": "off",
    "arrow-body-style": "off",
    "no-console": "warn",
    "prettier/prettier": "error"
  }
}
